---
# Warning messages in eventlog for AppLocker

- name: Members_local_admin_group
  hosts:
    - all
  serial:
    - 1
  #  strategy: free
  
  tasks:
    - name: Local_admins
      ansible.windows.win_powershell:
        script: |
          Get-LocalGroupMember -Group "Administratoren"
      register: members

    - name: print variable
      debug:
        msg: '{{ members.output }}'
